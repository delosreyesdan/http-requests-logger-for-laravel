{
  "name": "ddelosreyes/http-requests-logger-for-laravel",
  "description": "A simple HTTP request logger for Laravel with buffered inserts and support for both relational databases and DynamoDB.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ddelosreyes\\HttpRequestsLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ddelosreyes\\HttpRequestsLogger\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Daniel Delos Reyes",
      "email": "delosreyesdan@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "rector/rector": "^0.18",
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "pestphp/pest": "^3.8",
    "phpunit/phpunit": "^10.5|^11.5",
    "pestphp/pest-plugin-laravel": "^3.2"
  },
  "suggest": {
    "ext-redis": "Required if you want to use Redis as a buffer backend.",
    "aws/aws-sdk-php": "Required if you want to use DynamoDB as the storage driver."
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "vendor/bin/pest"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ddelosreyes\\HttpRequestsLogger\\Providers\\HttpRequestLoggerServiceProvider"
      ]
    }
  }
}
