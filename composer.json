{
    "name": "ddelosreyes/http-requests-logger-for-laravel",
    "description": "A simple HTTP request logger for Laravel with buffered inserts and support for both relational databases and dynamodb.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ddelosreyes\\HttpRequestsLogger\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Ddelosreyes\\HttpRequestsLogger\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Daniel Delos Reyes",
            "email": "delosreyesdan@hotmail.com"
        }
    ],
    "require": {
      "php": "^8.3",
      "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
      "ext-redis": "*"
    },
    "require-dev": {
      "rector/rector": "dev-main",
      "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0",
      "pestphp/pest": "^3.8",
      "phpunit/phpunit": "^9.0|^10.5|^11.5.3",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "suggest": {
      "aws/aws-sdk-php": "Required if you want to use DynamoDB as the storage driver."
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
  "scripts": {
    "test": "vendor/bin/pest"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ddelosreyes\\HttpRequestsLoggerForLaravel\\HttpRequestLoggerServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
